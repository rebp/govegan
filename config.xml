<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.rebp.govegan" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GoVegan</name>
    <description>
        An App made only for vegans.
    </description>
    <author email="r.e.bautista.prieto@hotmail.com" href="https://rebp.nl">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.1.2" />
    <plugin name="cordova-plugin-googlemaps" spec="~1.4.0">
        <variable name="API_KEY_FOR_ANDROID" value="AIzaSyBhi4Cz2-mTNtuQKtaQo5ZdS36ACCXuRlI" />
    </plugin>
    <plugin name="cordova-plugin-dialogs" spec="~1.3.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.7.0" />
    <plugin name="com.phonegap.plugins.barcodescanner" spec="https://github.com/Telerik-Verified-Plugins/BarcodeScanner" />
    <plugin name="com.telerik.plugins.nativepagetransitions" spec="https://github.com/Telerik-Verified-Plugins/NativePageTransitions" />
    <plugin name="cordova-plugin-chrome-apps-storage" spec="~1.0.4" />
    <plugin name="com.brodysoft.sqlitePlugin" spec="https://github.com/Telerik-Verified-Plugins/SQLite" />
</widget>
